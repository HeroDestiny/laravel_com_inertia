vendor/

# Ignore generated files:
*.min.js
*.min.css

# Ignore Laravel specific files:
bootstrap/cache/
storage/
.env*

# Ignore git files:
.git/
.gitignore

# Ignore docker files:
docker/
Dockerfile*

# Ignore OS files:
.DS_Store
Thumbs.db

# Ignore IDE files:
.vscode/
.idea/

# Ignore documentation that shouldn't be auto-formatted:
docs/development/UML_DIAGRAMS_clean.md
